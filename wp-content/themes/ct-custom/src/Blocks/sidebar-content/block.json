{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ct-custom/sidebar-content",
	"title": "Sidebar Content",
	"category": "ct-custom",
	"icon": "columns",
	"description": "Custom content for a page sidebar. Set the position to match your sidebar configuration.",
	"keywords": [ "sidebar", "aside", "left", "right" ],
	"attributes": {
		"position": {
			"type": "string",
			"enum": [ "left", "right" ],
			"default": "left"
		}
	},
	"supports": {
		"html": false,
		"reusable": false,
		"multiple": true
	},
	"editorScript": "file:./editor.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}
