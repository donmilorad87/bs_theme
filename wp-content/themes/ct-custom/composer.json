{
    "name": "ct-custom/theme",
    "description": "CT Custom WordPress theme dependencies",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "phpmailer/phpmailer": "^6.9",
        "firebase/php-jwt": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "CTCustom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CTCustom\\Tests\\Multilang\\": "tests/multilang/",
            "CTCustom\\Tests\\Auth\\": "tests/auth/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
